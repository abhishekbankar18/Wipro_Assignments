//private LocalDateTime orderDate; 
//Getters and Setters

public Order() {
	super();
	// TODO Auto-generated constructor stub
}

public Long getId() {
	return id;
}

public void setId(Long id) {
	this.id = id;
}

@Override
public String toString() {
	return "Order [id=" + id + "]";
} 


 


} 